datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @id @default(uuid())
  login     String
  password  String
  version   Int       @default(1)
  createdAt DateTime  @default(now())
  updatedAt DateTime  @default(now()) @updatedAt
}